{
  "name": "buna",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "buna": "./src/cli/index.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/indexts"
    },
    "./cli": {
      "import": "./src/cli/index.ts",
      "types": "./src/cli/index.ts"
    },
    "./client/layout-registry": {
      "import": "./src/client/layout-registry.ts",
      "types": "./src/client/layout-registry.ts"
    },
    "./cli/build": {
      "import": "./src/cli/build.ts",
      "types": "./src/cli/build.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "dev": [
        "dist/dev/generate-routes.d.ts"
      ],
      "dev/*": [
        "dist/dev/*"
      ],
      "cli": [
        "dist/cli/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.json --watch",
    "dev": "bun run watch",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "bun-plugin-tailwind": "^0.1.2",
    "typescript": "^5.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
