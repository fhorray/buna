{
  "name": "buna",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "buna": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dev": {
      "import": "./dist/dev/generate-routes.js",
      "types": "./dist/dev/generate-routes.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "dev": [
        "dist/dev/generate-routes.d.ts"
      ],
      "dev/*": [
        "dist/dev/*"
      ],
      "cli": [
        "dist/cli/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.json --watch"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
