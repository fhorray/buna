{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "buna": ["packages/buna/src"],
      "buna/*": ["packages/buna/src/*"],
      "#buna/*": ["apps/playground/.buna/*"],
      "@/buna.config": ["apps/playground/buna.config.ts"],
      "@/*": ["apps/playground/*"]
    }
  }
}
