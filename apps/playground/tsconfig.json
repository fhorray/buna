{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "#buna/*": [".buna/*"],
      "buna": ["../../packages/buna/src"],
      "buna/*": ["../../packages/buna/src/*"]
    }
  },
  "include": [
    "src",
    ".buna",
    "buna.config.ts",
    "types"
  ],
  "exclude": ["dist", "node_modules"]
}