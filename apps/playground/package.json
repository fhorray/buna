{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "buna:codegen": "bun run ../../packages/buna/src/cli/index.ts codegen",
    "dev": "bun run buna:codegen && bun run --hot entry.ts",
    "start": "NODE_ENV=production bun entry.ts",
    "build": "bun run build.ts"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.1.2",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.1.11",
    "buna": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
